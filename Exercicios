6-)
#include <stdio.h>

int main() {
    int soma = 0;
int i = 1;
while (i <= 50) {
  soma = soma + i;
  i++;
}
printf("A soma dos números de 1 a 50 é %d", soma);

    return 0;
}


7-)
#include <stdio.h>

int main() {
    int idade;
    printf("Informe sua idade:");
    scanf("%d",&idade);
    
    if(idade<12){
        printf("Você é criança");
    }
    if(idade>12 && idade<18){
        printf("Você é adolescente");
    }
    if(idade>18 && idade<60){
        printf("Você é adulto");
    }
    if(idade>60){
        printf("Você é idoso");
    }    
    return 0;
}


9-)
#include <stdio.h>

int main() {
    int a, b;
        printf("Digite um número:");
        scanf("%d", &a);
        
        printf("Digite um número:"); 
        scanf("%d", &b);
        
        if(a>b){
            printf("&a");
        }else{
            printf("&b");
        }

    return 0;
}
