/*lista de exercícios*/
#include <stdio.h>
10.
int main(){
    int n;
    printf("digite um número: ");
    scanf("%d", &n);
    if(n%2==0){
        printf("%d é par", n);
    }else{
        printf("%d é ímpar", n);
    }

    return 0;
11.
} 

/*lista de exercícios*/
#include <stdio.h>

int main(){
    int n = 1;
    while(n<=30){
        if(n%3==0){
            n++;
        }else{
            printf("%d \n", n);
            n++;
        }
    }

    return 0;
12.
}

/*lista de exercícios*/
#include <stdio.h>

int main(){
    int a;
    int b;
    printf("digite dois números:\n");
    scanf("%d\n%d", &a, &b);
    if(a==b){
        printf("os dois números são iguais");
    }else{
        printf("os dois números são diferentes");
    }
    
    return 0;
}

13.
